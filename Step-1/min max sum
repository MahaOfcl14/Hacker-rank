void miniMaxSum(vector<int> arr) {

   long long totSum = 0;
   int minSum = INT_MAX;
   int maxSum = INT_MIN;
   for(int num : arr) {
    totSum += num;
    minSum = min(minSum , num);
    maxSum = max(maxSum, num);
    
    
   }
   long long mini = totSum - maxSum;
   long long maxi = totSum - minSum;
   cout << mini << " " << maxi;
    
}
