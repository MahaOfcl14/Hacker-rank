void insertionSort2(int n, vector<int> arr) {
    for(int i=1; i<n; i++){
        for(int j=i-1;j>=0;j--){
            if(arr[j]>arr[j+1]){
            int num = arr[j+1];
            arr[j+1]=arr[j];
            arr[j]=num;
    }
    else break;
 }
    
    for(int k=0; k<n; k++) cout<<arr[k]<<" ";
    cout<<endl;
    }

}
